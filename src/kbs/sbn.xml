<g2:c.PROCEDURE ID="120258d387f011e7f7fa014703bc01ce">
  <g2:a.TEXT><![CDATA[
sbn-test-callback(id : integer, payload : class integer-array)
i: integer;
l: integer;
begin
    post "Id: [id], length: [the array-length of payload]";
    l = the array-length of payload;
    conclude that the array-length of sbn-demo-payload = l;
    l = l - 1;
    for i = 0 to l do
        change sbn-demo-payload[i]=payload[i]
    end;
end
]]></g2:a.TEXT>
