<g2:c.PROCEDURE ID="b71015e64c9411e78e810a458c4e7fd8">
  
  <g2:a.TEXT><![CDATA[
rpc-sync-call-test(node:class benchmark-test)
errobj: class error;
process-status: float;
success: truth-value;
text-file-stream : class g2-stream;
each-line: text;
operating-system: symbol; 
out-file:text;

begin
process-status = call start-test-app();
repeat
exit if the gsi-interface-status of rpc-interface = 2;
wait for 1 second;
if the gsi-interface-status of rpc-interface <= 0 then change the text of the gsi-connection-configuration of rpc-interface to "tcp-ip host @"localhost@" port-number 22066"; 
end;

success = false;
start rpc-action() across rpc-interface;
wait for 1 second;

    operating-system = call g2-type-of-file-system();
    if operating-system = symbol("WIN32") then
        out-file ="C:\G2\results\rpc4337.out"
    else
        out-file ="/gensym/results/rpc4337.out";
    text-file-stream = call g2-open-file-for-read(out-file);
    each-line = call g2-read-line(text-file-stream);
   if is-contained-in-text(each-line, "Synchronous call from G2FASTH") then success = true;
    call g2-close-file(text-file-stream);

wait for 1 second; 
start rpc-exit() across rpc-interface;
call g2-kill-process(process-status);
if success is false then call fail(node, " Synchronous call from G2FASTH failed")
else call pass(node); 
end 
]]></g2:a.TEXT>
