  <g2:a.TEXT><![CDATA[
g2-remote(str: text) = ()
operating-system: symbol; 
out-file: text;
text-file-stream: class g2-stream;
begin
    operating-system = call g2-type-of-file-system();
    if operating-system = symbol("WIN32") then
        out-file ="C:\G2\results\rpc4337.out"
    else
        out-file ="/gensym/results/rpc4337.out";
    text-file-stream = call g2-open-file-for-write(out-file); 
    call g2-write-line(text-file-stream, str);
    call g2-close-file(text-file-stream);
end

