<g2:c.PROCEDURE ID="d65f05bd4d5711e785260a458c4e7fd8">
  
  <g2:a.TEXT><![CDATA[
start-test-app() = (float)
operating-system: symbol;
process-status: float;
process-exists: truth-value;
begin
operating-system = call g2-type-of-file-system();
if operating-system = symbol("WIN32") then
process-status =
    call g2-spawn-process-to-run-command-line("C:\G2\src\g2fasth\bin\Release\rpc_test 22066>C:\G2\results\rpc4337.log")
else
process-status =
    call g2-spawn-process-to-run-command-line("/gensym/src/g2fasth/bin/rpc_test 22066>/gensym/results/rpc4337.log");
if process-status = -1 then
repeat
    wait for 1 second;
    process-exists = call g2-process-exists(process-status);
    exit if process-exists = false;
end;
return process-status;
end

]]></g2:a.TEXT>
