<g2:c.PROCEDURE ID="b8c6bd9346b911e78578005056236692">
  
  <g2:a.TEXT><![CDATA[
LOGBOOK-MESSAGE-TEST(node: class benchmark-test)
proc-ref: item-or-value ;
msg-in: text = "bad"; 
begin
    {Testing the registration of a handler}
    call g2-register-logbook-message-handler(logbook-message-handler);
    proc-ref = call g2-get-logbook-message-handler();
    if proc-ref is a truth-value then
    begin
        call fail (node, "g2-get-logbook-message-handler did not return a valid procedure after registration");  
        return;
    end;

    {checking if the procedure returned is really the procedure registered}
    if logbook-message-handler is not the same object as proc-ref then begin
        call fail(node, "LOGBOOK-MESSAGE-TEST: The original procedure is different from the returned one: g2-get-logbook-message-handler"); 
        return;
    end; 

    {Testing the triggering of the handler procedure}    
    post"[msg-in]"; 
    
    wait until the current value of flag-logbook-handler-msg  = true checking every 1 second;

    {Testing the deregistration of the handler procedure}
    call g2-deregister-logbook-message-handler (); 

    proc-ref = call g2-get-logbook-message-handler ();
    if  (proc-ref is not a truth-value) or (proc-ref = true) then begin
        call fail (node, " g2-deregister-logbook-message-handler did not clean the message handler procedure propperly"); 
        return;
    end;

    call pass(node);
end
]]></g2:a.TEXT>
