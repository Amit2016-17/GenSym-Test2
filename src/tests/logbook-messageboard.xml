<g2:c.PROCEDURE ID="2d3704e6454011e7842c005056236692">
  
  <g2:a.TEXT><![CDATA[
MESSAGE-BOARD-TEST(node: class benchmark-test)
proc-ref: item-or-value ;
msg-in: text = "bad";
begin

    {Testing the registration of a handler}
    call g2-register-message-board-message-handler(message-board-handler);
    proc-ref = call g2-get-message-board-message-handler();
    if proc-ref is a truth-value then
    begin
        call fail (node, "g2-get-message-board-message-handler did not return a valid procedure");  
        return;
    end;
    
    {checking if the procedure returned is really the procedure registered}
    if message-board-handler is not the same object as proc-ref then begin
        call fail(node, "MESSAGE-BOARD-TEST: The original procedure is different from the returned one: g2-get-message-board-message-handler "); 
        return;
    end;

    {Testing the triggering of the handler procedure}
    post"[msg-in]";
    wait until  the current value of flag-msgboard-handler-msg = true checking every 1 second;

    {Testing the deregistration of the handler procedure}
    call g2-deregister-message-board-message-handler ( ); 

    proc-ref = call g2-get-message-board-message-handler();
    if  (proc-ref is not a truth-value) or (proc-ref = true) then begin
        call fail (node, "g2-deregister-message-board-message-handler did not clean the message handler procedure propperly"); 
        return;
    end;
 
    call pass(node); 

end
]]></g2:a.TEXT>
