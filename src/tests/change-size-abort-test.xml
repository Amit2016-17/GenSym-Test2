<g2:c.PROCEDURE ID="fa23dd0e742a11e7bf6c000c29ebfb33">
  
  <g2:a.TEXT><![CDATA[
change-size-abort-test(node: class benchmark-node)

    err: class error;
    key-code: integer;

begin
    show sub-ws-of-test at the top left corner of the screen;
    call g2-player-reset();
    conclude that player-wait-time = 5;
    wait for 1 seconds;
    // select change-size
    call do-iconic-item-menu-selections(test-chart-inst, sequence("Change Size"));
    // type ENTER
    key-code = call g2-key-code-from-symbol (the symbol return);
    call g2-generate-event(the symbol keystroke,key-code,0,0); 
    call pass(node);
end on error(err)
    call fail(node, "[the error-description of err]");
end
]]></g2:a.TEXT>
