<g2:c.PROCEDURE ID="c7006a31666311e084ee000c2924e8e2">
  
  <g2:a.TEXT><![CDATA[
t-g2-reflect-items-test(node: class benchmark-node )

icon-ref, result_horz, result_vert : symbol;
icon-head :integer;

REFLECTED:symbol = the symbol REFLECTED;        
REFLECTED_90:symbol = the symbol REFLECTED-CLOCKWISE-90;        
REFLECTED_180:symbol = the symbol REFLECTED-CLOCKWISE-180;        
REFLECTED_270:symbol = the symbol REFLECTED-CLOCKWISE-270;    
LEFT_RIGHT:symbol = the symbol LEFT-RIGHT;

begin


//  1)  Icon-heading = 0;  icon-reflection  = none.

 
    // a)   reflected horizontally.
    result_horz  = call g2-reflect-item-horizontally(array_1);   
    icon-head = the icon-heading of array_1;
    icon-ref  = call get-icon-reflection(array_1); 

  //post "[result_horz] ---- [icon-ref] ---- [icon-head]";
    if result_horz /= REFLECTED then
        begin
        call fail(node, "The Expected value of  reflect horizontally is [REFLECTED] but the result was [result_horz]");
        return;    
    end;        
    if icon-head /= 0 then
        begin
        call fail(node, "The Expected value of icon-head is  0 but the result was [icon-head]");
        return;    
    end;        
    if icon-ref /= LEFT_RIGHT then
        begin
        call fail(node, "The Expected value of icon-reflection is [LEFT_RIGHT] but the result was [icon-ref]");
        return;    
    end;        





    // b)   reflected vertically.
    call g2-reflect-item-horizontally(array_1);   
    result_vert  = call g2-reflect-item-vertically(array_1);   
    icon-ref  = call get-icon-reflection(array_1); 
    icon-head = the icon-heading of array_1;

 //post "[result_vert] ---- [icon-ref] ---- [icon-head]";

    if result_vert /= REFLECTED_180 then
        begin
        call fail(node, "The Expected value of  reflect vertically is [REFLECTED_180] but the result was [result_vert]");
        return;    
    end;        
    if icon-head  /= 180 then
        begin
        call fail(node, "The Expected value of icon-head is  180 but the result was [icon-head]");
        return;    
    end;        
    if icon-ref  /= LEFT_RIGHT then
       begin
        call fail(node, "The Expected value of icon-reflection is [LEFT_RIGHT] but the result was [icon-ref]");
        return;    
    end;        



//  2)  Icon-heading = 90;  icon-reflection  = none.

      rotate array_2 by 90 degrees; 
 
    // a)   reflected horizontally.
    result_horz  = call g2-reflect-item-horizontally(array_2);   
    icon-head = the icon-heading of array_2;
    icon-ref  = call get-icon-reflection(array_2); 

 //post "[result_horz] ---- [icon-ref] ---- [icon-head]";
    if result_horz /= REFLECTED_270  then
        begin
        call fail(node, "The Expected value of  reflect horizontally is [REFLECTED_270] but the result was [result_horz]");
        return;    
    end;        
    if icon-head /= 270 then
        begin
        call fail(node, "The Expected value of icon-head is  270 but the result was [icon-head]");
        return;    
    end;        
    if icon-ref /= LEFT_RIGHT then
        begin
        call fail(node, "The Expected value of icon-reflection is [LEFT_RIGHT] but the result was [icon-ref]");
        return;    
    end;        





    // b)   reflected vertically.
    call g2-reflect-item-horizontally(array_2);   
    result_vert  = call g2-reflect-item-vertically(array_2);   
    icon-ref  = call get-icon-reflection(array_2); 
    icon-head = the icon-heading of array_2;

   //post "[result_vert] ---- [icon-ref] ---- [icon-head]";

    if result_vert /= REFLECTED_90 then
        begin
        call fail(node, "The Expected value of  reflect vertically is [REFLECTED_90] but the result was [result_vert]");
        return;    
    end;        
    if icon-head  /= 90 then
        begin
        call fail(node, "The Expected value of icon-head is  90 but the result was [icon-head]");
        return;    
    end;        
    if icon-ref  /= LEFT_RIGHT then
       begin
        call fail(node, "The Expected value of icon-reflection is [LEFT_RIGHT] but the result was [icon-ref]");
        return;    
    end;        



//  3)  Icon-heading = 180;  icon-reflection  = none.

      rotate array_3 by 180 degrees; 
 
    // a)   reflected horizontally.
    result_horz  = call g2-reflect-item-horizontally(array_3);   
    icon-head = the icon-heading of array_3;
    icon-ref  = call get-icon-reflection(array_3); 

   //post "[result_horz] ---- [icon-ref] ---- [icon-head]";
    if result_horz /= REFLECTED_180  then
        begin
        call fail(node, "The Expected value of  reflect horizontally is [REFLECTED_180] but the result was [result_horz]");
        return;    
    end;        
    if icon-head /= 180 then
        begin
        call fail(node, "The Expected value of icon-head is  180 but the result was [icon-head]");
        return;    
    end;        
    if icon-ref /= LEFT_RIGHT then
        begin
        call fail(node, "The Expected value of icon-reflection is [LEFT_RIGHT] but the result was [icon-ref]");
        return;    
    end;        





    // b)   reflected vertically.
    call g2-reflect-item-horizontally(array_3);   
    result_vert  = call g2-reflect-item-vertically(array_3);   
    icon-ref  = call get-icon-reflection(array_3); 
    icon-head = the icon-heading of array_3;

   //post "[result_vert] ---- [icon-ref] ---- [icon-head]";

    if result_vert /= REFLECTED then
        begin
        call fail(node, "The Expected value of  reflect vertically is [REFLECTED] but the result was [result_vert]");
        return;    
    end;        
    if icon-head  /= 0 then
        begin
        call fail(node, "The Expected value of icon-head is  0 but the result was [icon-head]");
        return;    
    end;        
    if icon-ref  /= LEFT_RIGHT then
       begin
        call fail(node, "The Expected value of icon-reflection is [LEFT_RIGHT] but the result was [icon-ref]");
        return;    
    end;        









//  4)  Icon-heading = 270;  icon-reflection  = none.

      rotate array_4 by 270 degrees; 
 
    // a)   reflected horizontally.
    result_horz  = call g2-reflect-item-horizontally(array_4);   
    icon-head = the icon-heading of array_4;
    icon-ref  = call get-icon-reflection(array_4); 

  //post "[result_horz] ---- [icon-ref] ---- [icon-head]";
    if result_horz /= REFLECTED_90  then
        begin
        call fail(node, "The Expected value of  reflect horizontally is [REFLECTED_90] but the result was [result_horz]");
        return;    
    end;        
    if icon-head /= 90 then
        begin
        call fail(node, "The Expected value of icon-head is  90 but the result was [icon-head]");
        return;    
    end;        
    if icon-ref /= LEFT_RIGHT then
        begin
        call fail(node, "The Expected value of icon-reflection is [LEFT_RIGHT] but the result was [icon-ref]");
        return;    
    end;        





    // b)   reflected vertically.
    call g2-reflect-item-horizontally(array_4);   
    result_vert  = call g2-reflect-item-vertically(array_4);   
    icon-ref  = call get-icon-reflection(array_4); 
    icon-head = the icon-heading of array_4;

  //post "[result_vert] ---- [icon-ref] ---- [icon-head]";

    if result_vert /= REFLECTED_270 then
        begin
        call fail(node, "The Expected value of  reflect vertically is [REFLECTED_270] but the result was [result_vert]");
        return;    
    end;        
    if icon-head  /= 270 then
        begin
        call fail(node, "The Expected value of icon-head is  270 but the result was [icon-head]");
        return;    
    end;        
    if icon-ref  /= LEFT_RIGHT then
       begin
        call fail(node, "The Expected value of icon-reflection is [LEFT_RIGHT] but the result was [icon-ref]");
        return;    
    end;        

  call pass(node);
end



]]></g2:a.TEXT>
