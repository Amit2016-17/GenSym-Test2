<g2:c.PROCEDURE ID="b71015e64c9411e78e810a458c4e7fd8">
  <g2:a.TEXT><![CDATA[
rpc-call-test(node:class benchmark-test)
errobj: class error;
process-status: float;
success1: truth-value;
success2: truth-value;
success3: truth-value;
float-result1: float;
float-result2: float;
text-result: text;
begin
process-status = call start-test-app();
repeat
exit if the gsi-interface-status of rpc-interface = 2;
wait for 1 second;
if the gsi-interface-status of rpc-interface <= 0 then change the text of the gsi-connection-configuration of rpc-interface to "tcp-ip host @"localhost@" port-number 22060"; 
end;

success1 = false;
success2 = false;
success3 = false;
begin
    float-result1 = call rpc-add-float(3, 5) across rpc-interface;
    if float-result1 = 8 then success1 = true;

    float-result2 = call rpc-add-float(1234.5678, 9876.5432) across rpc-interface ;
    if float-result2 = 11111.111 then success2 = true;
    text-result = call rpc-text("test") across rpc-interface;
    if text-result = "RPC text value" then success3 = true;

    if success1 is false then call fail(node, "Wrong value [float-result1] returned from rpc-add-float")
    else if success2 is false then call fail(node, "Wrong value [float-result2] returned from rpc-add-float")
    else if success3 is false then call fail(node, "Wrong value [text-result] returned from rpc-text")
    else call pass(node);
end
on error(errobj)
    call fail(node, "[the error-description of errobj]");
    delete errobj;
end;
start rpc-exit() across rpc-interface;
call g2-kill-process(process-status);
end 
]]></g2:a.TEXT>
