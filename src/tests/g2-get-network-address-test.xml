<g2:c.PROCEDURE ID="c7006a31666311e084ee000c2924e8e2">
  
  <g2:a.TEXT><![CDATA[
t-g2-get-network-address-test(node: class benchmark-node )
network-seq:sequence;
win32-seq : sequence = sequence();
unix-seq : sequence = sequence();
file-system : symbol;
process-status: float;
this-stream: class g2-stream;
this-line: text;
process-exists : truth-value;
address-length, colon:integer;
u-txt : text;
ipaddress : text;
tr-val : truth-value;

begin
 network-seq = call  G2-GET-NETWORK-ADDRESS-LIST();
 // post "[network-seq]";
 file-system = call g2-type-of-file-system();


 if file-system = the symbol win32  or
   file-system = the symbol dos  then
    begin
    		process-status = call g2-spawn-process-to-run-command-line(" ipconfig  > process-file");
    		repeat
     		    wait for 1 second;
    		    process-exists = call g2-process-exists(process-status);
    		    exit if process-exists = false;
    		end;
    		this-stream = call g2-open-file-for-read("process-file");
    		repeat
    		    exit if the g2-stream-status of this-stream is end-of-file-reached;
    		    this-line = call read-line(this-stream);
    		    if is-contained-in-text("IPv4", this-line) = true then
    		    	begin
    		    	address-length = length-of-text(this-line);
    		    	colon = position-of-text(":", this-line);
    		    	ipaddress = get-from-text(this-line, colon+2,address-length);
              win32-seq = insert-at-beginning(win32-seq , ipaddress);
            end;	
    		end;
    		call g2-close-file(this-stream);
        tr-val = call verify-sequence-equality(win32-seq, network-seq);
        if tr-val /= true then
          begin
          call fail(node, "Expected a network sequence from the command [network-seq] but the result was [win32-seq]")  
        end;
  end;

 if file-system = the symbol linux  or
    file-system = the symbol unix  then
    begin
    	  u-txt  = "ifconfig |  sed '/inet addr/!d;  s/^.*inet addr://;  s/ .*//; /127.0.0.1/d;'  > process-file";
    		process-status =  call g2-spawn-process-to-run-command-line(u-txt);
    		repeat
        		wait for 1 second;
    		    process-exists = call g2-process-exists(process-status);
    		    exit if process-exists = false;
    		end;
    		this-stream = call g2-open-file-for-read("process-file");
    		repeat
    		    exit if the g2-stream-status of this-stream is end-of-file-reached;
    		    this-line = call read-line(this-stream);
            unix-seq = insert-at-beginning(unix-seq , this-line);
         end;
    	  call g2-close-file(this-stream);
        tr-val = call verify-sequence-equality(unix-seq, network-seq);
        if tr-val /= true then
          begin
          call fail(node, "Expected a network sequence from the command [network-seq] but the result was [unix-seq]") 
        end;
    end;
 call pass(node);
end


]]></g2:a.TEXT>
