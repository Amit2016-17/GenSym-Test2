<g2:c.PROCEDURE ID="2b9dc453780b11e78e10005056236692">
  
  <g2:a.TEXT><![CDATA[
verify-sequence-equality(seq_1:sequence, seq_2 : sequence)=(truth-value)
I: text;
trval : truth-value;
begin
    if the number of elements in seq_1 /= the number of elements in seq_2 then
        return false;

    for I = each text in seq_1
    do
    if I is not  a member of seq_2 then
    	return false;
    end;	

    for I = each text in seq_2
    do
    if I is not  a member of seq_1 then
    	return false;
    end;	

    return true;
end




]]></g2:a.TEXT>
