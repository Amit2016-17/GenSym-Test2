  <g2:a.TEXT><![CDATA[
stop-profiling-in-testing(test-name: text)

    err-obj: class error;
    file-path: text;
    profiled-data: class system-profile-information;
    
begin
    
    if profiling-mode-configuration is do-not-use then
        return;
        
    file-path = "[test-name].procedure-coverage.txt";
    profiled-data = call g2-get-profiled-information();
    call write-profiling-results-to-file(file-path, profiled-data);
    
    call g2-disable-profiling();
    
end on error(err-obj)
    post "profiling tools error: [the class of err-obj] [the text of the error-description of err-obj]";
    delete err-obj;
end
]]></g2:a.TEXT>
