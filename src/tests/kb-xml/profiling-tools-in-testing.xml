  <g2:a.TEXT><![CDATA[
write-profiling-results-to-file(file-path: text, profiled-data: class system-profile-information)

    err-obj: class error;
    stream: class g2-stream;
    auto-test-dir-exist: truth-value;
    auto-test-dir: text;
    profiled-items: class item-list;
    item-profile-info: class item-profile-information;
    profiled-item: class item;
    item-type: text;
    item-name: value;

begin

    auto-test-dir = call g2-get-environment-variable ("G2_AUTO_TEST") ;
    auto-test-dir-exist = call g2-directory-exists(auto-test-dir);
    if auto-test-dir-exist and auto-test-dir /= "" then begin
        call g2-change-default-directory (auto-test-dir);
    end;
    
    stream = call g2-open-file-for-write(file-path);
   
    profiled-items = the profiled-items of profiled-data;
    for item-profile-info = each item-profile-information in profiled-items do
        profiled-item = the item that is profiled-by item-profile-info;
        item-type = "none";
        if profiled-item is a procedure then
            item-type = "procedure";
        if profiled-item is a method then
            item-type = "method";
        if item-type /= "none" then begin
            item-name = call g2-name-for-item(profiled-item);
            call g2-write-line(stream, "[item-name],[item-type],[the calls of item-profile-info]");
        end;
    end;
    
    call g2-close-file(stream);
end on error(err-obj)
    post "profiling tools error: [the class of err-obj] [the text of the error-description of err-obj]";
    delete err-obj;
end
]]></g2:a.TEXT>
