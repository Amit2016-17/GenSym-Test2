  <g2:a.TEXT><![CDATA[
activate-profiling-tools()
    
    err-obj: class error;
    mode: text;
    
begin
    mode = call g2-get-environment-variable("G2-COLLECT-PROFILING-INFO");

    if mode = "" then
        conclude that profiling-mode-configuration = the symbol do-not-use
    else
        conclude that profiling-mode-configuration = symbol(mode);
    
    call start-profiling-in-testing();
    
end on error(err-obj)
    post "profiling tools error: [the class of err-obj] [the text of the error-description of err-obj]";
    delete err-obj;
end
]]></g2:a.TEXT>
