clean : 
	- del *.obj
	- del *.lib
	- del *.exe

install : 
	- echo y | rmdir /s $(TAPEDST)
	mkdir $(TAPEDST)
	mkdir $(WWWDST)
	mkdir $(KBDST)
	echo run-gw.bat >> $(CONTENTS)
	copy  $(PROMDIR)\run-gw.bat $(TAPEDST)\run-gw.bat
	echo gw.exe >> $(CONTENTS)
	copy  gw.exe $(TAPEDST)\gw.exe
	echo www\bridge.gif >> $(CONTENTS)
	copy  $(PROMDIR)\bridge.gif $(WWWDST)\bridge.gif
	echo kbs\gwex1.kb >> $(CONTENTS)
	copy  $(KBDIR)\gwex1.kb $(KBDST)\gwex1.kb
	echo kbs\gwex2.kb >> $(CONTENTS)
	copy  $(KBDIR)\gwex2.kb $(KBDST)\gwex2.kb
	echo kbs\gwex3.kb >> $(CONTENTS)
	copy  $(KBDIR)\gwex3.kb $(KBDST)\gwex3.kb
	echo kbs\gwlow.kb >> $(CONTENTS)
	copy  $(KBDIR)\gwlow.kb $(KBDST)\gwlow.kb
