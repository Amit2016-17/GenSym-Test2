	@- $(SCRIPTSDIR)\errsweep.bat rpcmn3.obj
	$(cc) -Gs -Ob1 -Og -Oi -Ot -Zi $(CC_FLAGS) -I$(RTHDIR) -I$(EXTCDIR) -I$(GSICDIR) $(CDIR)\rpcmn3.c
	@- del current.bat

