clean : 
	- rm -f *.[oa]
	- rm -f g2

install : 
	- rm -rf $(TAPEDST)/*
	- rm -rf $(TESTDST)/*
	mkdir $(EXTDST)
	chmod ug+r+w $(EXTDST)
	echo "ext/fgntest.c   `sum $(PROMDIR)/fgntest.c` " >> $(CONTENTS)
	cp -p $(PROMDIR)/fgntest.c $(EXTDST)/fgntest.c
	chmod 444 $(EXTDST)/fgntest.c
	echo "index.dic   `sum $(PROMDIR)/index.dic` " >> $(CONTENTS)
	cp -p $(PROMDIR)/index.dic $(TAPEDST)/index.dic
	chmod 444 $(TAPEDST)/index.dic
	echo "kojin.dic   `sum $(PROMDIR)/kojin.dic` " >> $(CONTENTS)
	cp -p $(PROMDIR)/kojin.dic $(TAPEDST)/kojin.dic
	chmod 444 $(TAPEDST)/kojin.dic
	echo "main.dic   `sum $(PROMDIR)/main.dic` " >> $(CONTENTS)
	cp -p $(PROMDIR)/main.dic $(TAPEDST)/main.dic
	chmod 444 $(TAPEDST)/main.dic
	echo "g2passwd   `sum $(EXTDIR)/g2passwd` " >> $(CONTENTS)
	cp -p $(EXTDIR)/g2passwd $(TAPEDST)/g2passwd
	chmod ug+r-w $(TAPEDST)/g2passwd
	echo "hostlookup   `sum $(EXTDIR)/hostlookup` " >> $(CONTENTS)
	cp -p $(EXTDIR)/hostlookup $(TAPEDST)/hostlookup
	chmod ug+r-w $(TAPEDST)/hostlookup
	echo "ext/overlay   `sum $(EXTDIR)/overlay` " >> $(CONTENTS)
	cp -p $(EXTDIR)/overlay $(EXTDST)/overlay
	chmod ug+r-w $(EXTDST)/overlay
	echo "g2   `sum g2` " >> $(CONTENTS)
	cp -p g2 $(TAPEDST)/g2
	chmod ug+r-w $(TAPEDST)/g2
	echo "ext/foreign.h   `sum $(EXTCDIR)/foreign.h` " >> $(CONTENTS)
	cp -p $(EXTCDIR)/foreign.h $(EXTDST)/foreign.h
	chmod 444 $(EXTDST)/foreign.h
	echo "ext/icp.h   `sum $(EXTCDIR)/icp.h` " >> $(CONTENTS)
	cp -p $(EXTCDIR)/icp.h $(EXTDST)/icp.h
	chmod 444 $(EXTDST)/icp.h
	echo "ext/libforgn.a   `sum $(EXTDIR)/libforgn.a` " >> $(CONTENTS)
	cp -p $(EXTDIR)/libforgn.a $(EXTDST)/libforgn.a
	chmod 444 $(EXTDST)/libforgn.a
	echo "ext/makefile   `sum $(MFPROMDIR)/makefgn-sparcsol` " >> $(CONTENTS)
	cp -p $(MFPROMDIR)/makefgn-sparcsol $(EXTDST)/makefile
	chmod 444 $(EXTDST)/makefile
	echo "g2.ok   `sum $(PROMDIR)/g2.ok` " >> $(CONTENTS)
	cp -p $(PROMDIR)/g2.ok $(TAPEDST)/g2.ok
	chmod 664 $(TAPEDST)/g2.ok
	echo "ext/fgntest.tpl   `sum $(PROMDIR)/fgntest.tpl` " >> $(CONTENTS)
	cp -p $(PROMDIR)/fgntest.tpl $(EXTDST)/fgntest.tpl
	chmod 444 $(EXTDST)/fgntest.tpl
	cp -rp $(TAPEDST)/* $(TESTDST)
	rm -f $(TESTDST)/g2.ok
