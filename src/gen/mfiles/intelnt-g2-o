	@- $(SCRIPTSDIR)\errsweep.bat g2.exe
	$(link) @<<
  -out:g2.exe $(ldebug) $(lprofile) $(guilflags) -MAP:g2.map -STACK:8000000 /nodefaultlib:libc.lib /nodefaultlib:libcmt.lib /PDB:NONE /machine:IX86  winmain.obj main.obj symtab.obj userfuns.obj userinit.obj usersyms.obj names.obj load.obj btstrp.obj systm.obj delta.obj loop.obj g2pts.obj glbscs.obj osfrgn.obj basics.obj ostme.obj os-set.obj os-mem.obj prmtvs.obj chars.obj jismap.obj ksmap2.obj gbmap.obj gbmapr.obj chars2.obj kybrds.obj kydfs.obj utlts0.obj utlts1.obj utlts2.obj lttrs.obj uncde.obj is2022.obj trnscd.obj is8859.obj alphbt.obj token.obj utlts3.obj utlts4.obj times.obj f-keys.obj osrrr.obj ptfms.obj launch.obj pthnms.obj flprse.obj strms.obj ps.obj schdle.obj clocks.obj ntwrks.obj int0.obj int1.obj ntrrr.obj ntvnt.obj int2.obj int3a.obj int3b.obj int4.obj gscmn.obj win1.obj win2.obj win3.obj win3a.obj win4.obj lspimg.obj mice.obj fonts.obj icons.obj hb72.obj hm14.obj hm18.obj hm24.obj slots.obj slvc14.obj slvc18.obj slvc24.obj hbrw14.obj hbrw18.obj hbrw24.obj thai14.obj thai18.obj thai24.obj parse.obj parse2.obj frms1.obj types.obj frms2.obj frms2a.obj frms2b.obj frms3.obj frms4.obj frms5.obj panes.obj draw.obj entts.obj places.obj vw-def.obj mods.obj debug.obj pshdr.obj prntng.obj cnct1.obj cnct2.obj cnct3.obj cnct3a.obj cnct4.obj cnct5.obj dfntns.obj rltns.obj frmts.obj sqncs1.obj sqncs2.obj sqncs3.obj boxes.obj lngge.obj tables.obj telesb.obj dlogs.obj natvtw.obj wrkstn.obj books.obj menus.obj nwmns.obj fclts.obj kfep1.obj kfep2.obj kfep3.obj hfep1.obj hfep2.obj edit1.obj edit2.obj rings.obj moves.obj run.obj kbmrge.obj kbld1.obj kbld2.obj kbsv1.obj kbsv2.obj kbsv3.obj kbsv4.obj wrmbt.obj fntfls.obj cmptls.obj cmpl1.obj cmpl2.obj sets.obj lists.obj arrays.obj hshtbl.obj priqus.obj vrbls.obj hstrs.obj roles.obj chnng.obj eval.obj rules.obj nwrls.obj fnctns.obj cells.obj pcgrm.obj rpcmn1.obj rpcmn2.obj rpcmn3.obj pctls.obj pccmp.obj pcdbg.obj chglog.obj flsp.obj mem-sp.obj g2rpc1.obj g2rpc2.obj g2rpc3.obj expln.obj parts.obj dsplys.obj grphs1.obj grphs