install : 
	- rm -rf $(TAPEDST)/*
	- rm -rf $(TESTDST)/*
	echo "alarmer.c   `sum $(CDIR)/alarmer.c` " >> $(CONTENTS)
	cp -p $(CDIR)/alarmer.c $(TAPEDST)/alarmer.c
	chmod 444 $(TAPEDST)/alarmer.c
	echo "filetest.c   `sum $(CDIR)/filetest.c` " >> $(CONTENTS)
	cp -p $(CDIR)/filetest.c $(TAPEDST)/filetest.c
	chmod 444 $(TAPEDST)/filetest.c
	echo "gsi_main.c   `sum $(CDIR)/gsi_main.c` " >> $(CONTENTS)
	cp -p $(CDIR)/gsi_main.c $(TAPEDST)/gsi_main.c
	chmod 444 $(TAPEDST)/gsi_main.c
	echo "gsi_misc.c   `sum $(CDIR)/gsi_misc.c` " >> $(CONTENTS)
	cp -p $(CDIR)/gsi_misc.c $(TAPEDST)/gsi_misc.c
	chmod 444 $(TAPEDST)/gsi_misc.c
	echo "itemtest.c   `sum $(CDIR)/itemtest.c` " >> $(CONTENTS)
	cp -p $(CDIR)/itemtest.c $(TAPEDST)/itemtest.c
	chmod 444 $(TAPEDST)/itemtest.c
	echo "mapchar.c   `sum $(CDIR)/mapchar.c` " >> $(CONTENTS)
	cp -p $(CDIR)/mapchar.c $(TAPEDST)/mapchar.c
	chmod 444 $(TAPEDST)/mapchar.c
	echo "matrix.c   `sum $(CDIR)/matrix.c` " >> $(CONTENTS)
	cp -p $(CDIR)/matrix.c $(TAPEDST)/matrix.c
	chmod 444 $(TAPEDST)/matrix.c
	echo "pub_subscribe.c   `sum $(CDIR)/pub_subscribe.c` " >> $(CONTENTS)
	cp -p $(CDIR)/pub_subscribe.c $(TAPEDST)/pub_subscribe.c
	chmod 444 $(TAPEDST)/pub_subscribe.c
	echo "reposit.c   `sum $(CDIR)/reposit.c` " >> $(CONTENTS)
	cp -p $(CDIR)/reposit.c $(TAPEDST)/reposit.c
	chmod 444 $(TAPEDST)/reposit.c
	echo "rpcfuncs.c   `sum $(CDIR)/rpcfuncs.c` " >> $(CONTENTS)
	cp -p $(CDIR)/rpcfuncs.c $(TAPEDST)/rpcfuncs.c
	chmod 444 $(TAPEDST)/rpcfuncs.c
	echo "skeleton.c   `sum $(CDIR)/skeleton.c` " >> $(CONTENTS)
