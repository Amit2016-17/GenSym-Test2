SHELL = /bin/sh
PRODUCT = ext
ODIR = opt
PLATFORM = sun4
CDIR = $(SRCBOX)/$(PRODUCT)/c
HELPERDIR = /home/development/helpers
JPEGCDIR = $(SRCBOX)/jpeg/c
MFDIR = $(DSTBOX)/$(PRODUCT)/$(ODIR)
SCRIPTSDIR = /home/development/scripts
SPFCDIR = $(SRCBOX)/specific/$(PLATFORM)
CC_FLAGS = -Wimplicit

all : _mfenter g2libs backtrac.o lucidsrv.o nsleep.o g2passwd hostlookup overlay _mfexit

g2libs : libcprim.a libforgn.a libgsec.a libnogsec.a libtcp.a

_mfenter : 
	@- $(SCRIPTSDIR)/errsweep -init

_mfexit : 
	@- $(SCRIPTSDIR)/errsweep -finish

libcprim.a : $(SPFCDIR)/backtrac.o $(SPFCDIR)/nsleep.o clipboard.o \
  cprim.o cprocess.o cstreams.o ctime.o decnet.o dtoa.o event.o events.o \
  g2ptrs.o g2signal.o g_fmt.o graphics.o icmp.o images.o keypress.o md5_all.o \
  networks.o process.o sckevent.o sslnoop.o tcp.o trace.o windows.o \
  xwindows.o
	@ $(HELPERDIR)/wait-for-helpers -udir $(HELPERDIR)
	@- if [ -f libcprim.a ] ; then rm -f libcprim.a ; fi
	ar qc libcprim.a  cprim.o cprocess.o cstreams.o ctime.o \
  decnet.o dtoa.o events.o event.o g2ptrs.o g2signal.o g_fmt.o graphics.o \
  clipboard.o images.o md5_all.o networks.o sslnoop.o trace.o process.o tcp.o \
  icmp.o windows.o sckevent.o keypress.o xwindows.o $(SPFCDIR)/backtrac.o \
  $(SPFCDIR)/nsleep.o
	ranlib libcprim.a

libforgn.a : $(SPFCDIR)/backtrac.o cprim.o cprocess.o ctime.o \
  decnoop.o event.o extensn.o fgnevent.o foreign.o g2ptrs.o icp.o netstubs.o \
  networks.o pools.o sckevent.o sslnoop.o tcp.o trace.o
	@ $(HELPERDIR)/wait-for-helpers -udir $(HELPERDIR)
	@- if [ -f libforgn.a ] ; then rm -f libforgn.a ; fi
	ar qc libforgn.a  cprim.o cprocess.o ctime.o extensn.o \
  foreign.o g2ptrs.o icp.o networks.o trace.o pools.o fgnevent.o decnoop.o \
  tcp.o sslnoop.o netstubs.o event.o sckevent.o $(SPFCDIR)/backtrac.o
	ranlib libforgn.a

libgsec.a : 
	@ $(HELPERDIR)/wait-for-helpers -udir $(HELPERDIR)
	@- if [ -f libgsec.a ] ; then rm -f libgsec.a ; fi
	ar qc libgsec.a  
	ranlib libgsec.a

libnogsec.a : sslnoop.o
	@ $(HELPERDIR)/wait-for-helpers -udir $(HELPERDIR)
	@- if [ -f libnogsec.a ] ; then rm -f libnogsec.a ; fi
	ar qc libnogsec.a  sslnoop.o
	ranlib libnogsec.a

libtcp.a : $(SPFCDIR)/backtrac.o cprim.o cprocess.o cstreams.o \
  ctime.o decnoop.o event.o g2ptrs.o g2signal.o netstubs.o networks.o \
  sckevent.o tcp.o trace.o
	@ $(HELPERDIR)/wait-for-helpers -udir $(HELPERDIR)
	@- if [ -f libtcp.a ] ; then rm -f libtcp.a ; fi
	ar qc libtcp.a  cprim.o cprocess.o cstreams.o ctime.o \
  decnoop.o event.o g2ptrs.o g2signal.o networks.o trace.o tcp.o netstubs.o \
  sckevent.o $(SPFCDIR)/backtrac.o
	ranlib libtcp.a

g2passwd : g2passwd.o g2pwaux.o md5_all.o
	@ $(HELPERDIR)/wait-for-helpers -udir $(HELPERDIR)
	@- if [ -f g2passwd ] ; then rm -f g2passwd ; fi
	gcc -o g2passwd   g2passwd.o g2pwaux.o md5_all.o -lm -lc -ldl

hostlookup : hostlookup.o
	@ $(HELPERDIR)/wait-for-helpers -udir $(HELPERDIR)
	@- if [ -f hostlookup ] ; then rm -f hostlookup ; fi
	gcc -o hostlookup   hostlookup.o -lm -lc -ldl

overlay : overlay.o
	@ $(HELPERDIR)/wait-for-helpers -udir $(HELPERDIR)
	@- if [ -f overlay ] ; then rm -f overlay ; fi
	gcc -o overlay   overlay.o -lm -lc -ldl

backtrac.o : $(CDIR)/backtrac.c 
	@- if [ -f backtrac.o ] ; then rm -f backtrac.o ; fi
	@ $(HELPERDIR)/help-please $(MFDIR) gcc -c -O $(CC_FLAGS)  $(CDIR)/backtrac.c

clipboard.o : $(CDIR)/clipboard.c $(CDIR)/cprim.h $(CDIR)/graphics.h
	@- if [ -f clipboard.o ] ; then rm -f clipboard.o ; fi
	@ $(HELPERDIR)/help-please $(MFDIR) gcc -c -O $(CC_FLAGS)  $(CDIR)/clipboard.c

