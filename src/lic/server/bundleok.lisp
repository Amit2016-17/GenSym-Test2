(defstruct bundle-info
  name n-auths start-mo start-yr duration-in-mo
  machine-id products process-limit tw-floaters tw2-floaters packages)


(defun make-temp-bundle-ok-file (bundle-info)
  (loop with interval-data
	  = (make-interval-data
	      (bundle-info-n-auths bundle-info)
	      (bundle-info-start-mo bundle-info)
	      (bundle-info-start-yr bundle-info)
	      (bundle-info-duration-in-mo bundle-info))
	for ((nil nil nil)
	     (nil nil nil)
	     (start-day symbolic-start-month start-year)
	     (end-day symbolic-end-month end-year))
	    in interval-data
	as file-name = (format
			 nil
			 "~(~a~)-~d-v~d-~(~a~)~d-~d.ok"
			 (bundle-info-name bundle-info)
			 (bundle-info-tw2-floaters bundle-info)
			 default-ok-file-version-format
			 symbolic-end-month
			 end-day
			 end-year)
	do (multiple-value-bind
	       (code-1 code-2 code-3 code-4 code-5)
	       (user::make-ok
		 (bundle-info-machine-id bundle-info)
		 default-ok-file-version-format
		 (bundle-info-products bundle-info)
		 ;; notice that the order is end-date then start-date
		 `(,end-day ,symbolic-end-month ,end-year)
		 `(,start-day ,symbolic-start-month ,start-year)
		 (bundle-info-process-limit bundle-info)
		 (bundle-info-tw-floaters bundle-info)
		 (bundle-info-tw2-floaters bundle-info))
	     (with-open-file (output-stream file-name :direction :output :if-exists :supersede)
	       (print (truename output-stream))
	       (format output-stream
		       bundle-template-ok-file-format-string-1
		       (bundle-info-name bundle-info)
		       (bundle-info-machine-id bundle-info)
		       (bundle-info-products bundle-info)
		       (format nil
			       bundle-template-expiration-template-format-string
			       start-day symbolic-start-month start-year
			       end-day symbolic-end-month end-year)
		       code-1 code-2 code-3 code-4 code-5)
	       (emit-bundle-package-auths
		 output-stream
		 (bundle-info-machine-id bundle-info)
		 (bundle-info-packages bundle-info)
		 `(,start-day ,symbolic-start-month ,start-year)
		 `(,end-day ,symbolic-end-month ,end-year))
	       (format output-stream
		       bundle-template-ok-file-format-string-2
		       (bundle-info-process-limit bundle-info)
		       (bundle-info-tw-floaters bundle-info)
		       (bundle-info-tw2-floaters bundle-info))))))

(defun make-perm-bundle-ok-file (bundle-info)
  (let ((file-name (format
		     nil
		     ;; e.g., "g2-deployment-12-v5.ok"
		     ;; or "g2-development-6-v8-2-process.ok"
		     "~(~a~)-~d-v~d~a.ok"
		     (bundle-info-name bundle-info)
		     (bundle-info-tw2-floaters bundle-info)
		     default-ok-file-version-format
		     (if (> (bundle-info-process-limit bundle-info) 1)
			 (format nil "-~a-process"
				 (bundle-info-process-limit bundle-info))
			 ""))))
    (multiple-value-bind
	(code-1 code-2 code-3 code-4 code-5)
	(user::make-ok
	  (bundle-info-machine-id bundle-info)
	  default-ok-file-version-format
	  (bundle-info-products bundle-info)
	  nil nil
	  (bundle-info-process-limit bundle-info)
	  (bundle-info-tw-floaters bundle-info)
	  (bundle-info-tw2-floaters bundle-info))
      (with-open-file (output-stream file-name :direction :output :if-exists :supersede)
	(print (truename output-stream))
	(format output-stream
		bundle-template-ok-file-format-string-1
		(bundle-info-name bundle-info)
		(bundle-info-machine-id bundle-info)
		(bundle-info-products bundle-info)
		(format nil bundle-template-null-expiration-format-string)
		code-1 code-2 code-3 code-4 code-5)
	(emit-bundle-package-auths
	  output-stream
	  (bundle-info-machine-id bundle-info)
	  (bundle-info-packages bundle-info))
	(format output-stream
		bundle-template-ok-file-format-string-2
		(bundle-info-process-limit bundle-info)
		(bundle-info-tw-floaters bundle-info)
		(bundle-info-tw2-floaters bundle-info))))))


(defun make-bundle-ok-file (bundle-info)
  (if (bundle-info-n-auths bundle-info)
      (make-temp-bundle-ok-file bundle-info)
      (make-perm-bundle-ok-file bundle-info)))

(defun emit-bundle-package-auths (output-stream machine-id package-list &optional start-date? end-date?)
  (let ((ok (ab::make-frame 'ok))
	encoded-start-date encoded-end-date encoded-interval)
    (setf (ab::machine-id ok) (stringw machine-id))
    (when start-date?
      (setq encoded-start-date
	    (user::convert-input-to-universal-date-offline start-date?))
      (setq encoded-end-date
	    (user::convert-input-to-universal-date-offline end-date?))
      (setq encoded-interval
	    (ab::encode-authorization-interval encoded-start-date encoded-end-date)))
    (loop for package in package-list
          do (if start-date?
		 (ab::authorize-package ok package encoded-start-date encoded-end-date)
		 (ab::authorize-package ok package)))
    (loop for x in (reverse (ab::authorized-kb-packages ok))
	  as newline-p = nil then t
	  when newline-p
	    do (format output-stream "~%    ")
	      ;; X looks like this:
	      ;;
	      ;;   (gensym-gda4 386937 320421 346070) or (gensym-gda4 175698 502940 85801)
	      ;;
	      ;; We want output like this:  (if temporary)
	      ;;
	      ;;   (gensym-gda4  from 13 dec 2001 to 27 dec 2001  386937 320421 346070)
	      ;;
	      ;; or this:  (if permanent)
	      ;;
	      ;;   (gensym-gda4 175698 502940 85801)
	  do (if start-date?
		 (format output-stream "(~(~a  from ~d ~a ~d to ~d ~a ~d  ~d ~d ~d~))"
			 (first x)	; package name
			 (first start-date?) (second start-date?) (third start-date?) ; start
			 (first end-date?) (second end-date?) (third end-date?)	; end
			 (second x) (third x) (fourth x)) ; codes
		 (format output-stream "~(~a~)" x))) ; print the whole list
    (ab::delete-frame ok)))





