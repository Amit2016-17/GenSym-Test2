(defparameter months '(jan feb mar apr may jun jul aug sep oct nov dec))

(defun make-interval-data (number-of-auths start-month-symbol start-year duration-in-months)
  (loop with year = start-year
	with start-day = 25 
	with end-day = 25
	with fixed-duration
	  = (if (> end-day start-day) (- duration-in-months 1) duration-in-months)
	as zero-based-start-month
	   = (position start-month-symbol months)
	   then (mod (+ zero-based-start-month 1) 12)
	as symbolic-start-month = (nth zero-based-start-month months)
	as start-month = (+ zero-based-start-month 1)
	as zero-based-end-month
	   = (mod (+ zero-based-start-month fixed-duration) 12)
	as symbolic-end-month = (nth zero-based-end-month months)
	as end-month = (+ zero-based-end-month 1)
	as end-year = (+ year
			 (cond ((>= (+ zero-based-start-month duration-in-months) 24) 2)
			       ((>= (+ zero-based-start-month duration-in-months) 12) 1)
			       (t 0)))
	repeat number-of-auths
	collect `((,start-day ,start-month ,year)
		  (,end-day ,end-month ,end-year)
		  (,start-day ,symbolic-start-month ,year)
		  (,end-day ,symbolic-end-month ,end-year))
	when (= start-month 12)
	  do (setq year (+ year 1))))

(defparameter n-auths 15)
(defparameter start-mo 'dec)
(defparameter start-yr 2015)
(defparameter duration-in-mo 2) ; Paaras said "1 month"
(defparameter template-ok-file-name-format-string
  ;; args: symbolic-end-month, end-year
  "g2-development-1-v12-~(~a~)25-~d.ok")
(defparameter template-e-scor-professional-ok-file-name-format-string
  ;; args: symbolic-end-month, end-year
  "escor-1-v5-~(~a~)25-~d.ok")
(defparameter template-auth-command-line-format-string
  ;; args: start-day, start-month, start-year, end-day, end-month, end-year
  "/gensym/src/lic/licensing-client -u bhyde -p susan 'license G2-V12 \"Site: G2 Development\" (~d ~d ~d) (~d ~d ~d) 1 1 1 (online jl)')")

(defun do-it (&optional (family 'g2))
  (loop with interval-data
	  = (make-interval-data n-auths start-mo start-yr duration-in-mo)
	for ((nil nil nil)
	     (nil nil nil)
	     (start-day symbolic-start-month start-year)
	     (end-day symbolic-end-month end-year))
	    in interval-data
	as file-name = (format
			 nil 
			 (case family
			   (g2 template-ok-file-name-format-string)
			   (e-scor template-e-scor-professional-ok-file-name-format-string)
			   (t (cerror "continue" "Unknown family ~a!" family)))
			 symbolic-end-month end-year)
	do (multiple-value-bind
	       (code-1 code-2 code-3 code-4 code-5)
	       (user::make-ok
		 (case family
		   (g2 "SITE:G2DEVELOPMENT")
		   (e-scor "Site: e-SCOR Professional")
		   (t (cerror "continue" "Unknown family ~a!" family)))
		 default-ok-file-version-format 
		 (case family
		   (g2 '(online jl))
		   (e-scor '(online jl embedded))
		   (t (cerror "continue" "Unknown family ~a!" family)))
		 ;; notice that the order is end-date then start-date
		 `(,end-day ,symbolic-end-month ,end-year)
		 `(,start-day ,symbolic-start-month ,start-year)
		 1
		 (case family
		   (g2 1)
		   (e-scor 0)
		   (t (cerror "continue" "Unknown family ~a!" family)))
		 1)
	     (with-open-file (output-stream file-name :direction :output :if-exists :supersede)
	       (print (truename output-stream))
	       (format output-stream
		       (case family
			 (g2 template-ok-format-string)
			 (e-scor template-e-scor-professional-ok-format-string)
			 (t (cerror "continue" "Unknown family ~a!" family)))
		       start-day symbolic-start-month start-year
		       end-day symbolic-end-month end-year
		       code-1 code-2 code-3 code-4 code-5)))))





