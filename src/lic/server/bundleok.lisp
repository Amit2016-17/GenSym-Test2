;;; Change the following number to 5 or 8 or 83 as necessary.
;;; 83 is for 8.3b0 or later. - jpg 12/7/06
;;; 11 is for 2011. - binghe, Nov 13, 2013
;;; 12 is for 2015. - binghe, Nov 25, 2015
(defconstant default-ok-file-version-format 12)

(cond ((>= default-ok-file-version-format 12)
       (setq user::reference-date-for-interval-encoding user::jan-1-2015))
      ((or (>= default-ok-file-version-format 9) ; include 11
           (= default-ok-file-version-format 83))
       (setq user::reference-date-for-interval-encoding user::jan-1-2006))
      ((>= default-ok-file-version-format 5)
       (setq user::reference-date-for-interval-encoding user::jan-1-1998))
      (t
       (setq user::reference-date-for-interval-encoding user::jan-1-1990)))

